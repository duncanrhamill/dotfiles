{
   "author": "Creaktif",
   "background": {
      "persistent": false,
      "scripts": [ "config.js", "app.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "assets/19x19.png",
         "38": "assets/38x38.png"
      },
      "default_title": "Fontface Ninja"
   },
   "content_security_policy": "script-src 'self' https://www.google-analytics.com https://connect.facebook.net/; object-src 'self'",
   "default_locale": "en",
   "description": "Explore fonts within a website, try and buy them !",
   "homepage_url": "http://www.fontface.ninja",
   "icons": {
      "128": "assets/128x128.png",
      "16": "assets/16x16.png",
      "48": "assets/48x48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Fontface Ninja",
   "permissions": [ "http://*/", "https://*/", "tabs", "activeTab", "storage", "cookies" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.1.0",
   "web_accessible_resources": [ "src/css/inject.css", "src/js/inject.js" ]
}
